    "format": "prettier --write src/"
  },
  "devDependencies": {
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0"
  },
  "dependencies": {
    "abitype": "^0.7.1",
    "svelte": "^3.58.0",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "@wagmi/core": "^0.10.9"
  }
}
